### Copy this file to `.env` and fill in values for local development.
### THIS FILE MUST NOT CONTAIN REAL SECRETS IN VERSION CONTROL.

# Discord bot token
TOKEN=

# OAuth client ID
CLIENT_ID=

# OAuth client secret
DISCORD_CLIENT_SECRET=

# Session secret for express-session
SESSION_SECRET=

# Base URL (e.g., https://example.com or http://localhost:5000)
BASE_URL=
